<?xml version="1.0" encoding="utf-8"?>
<Container version="2">
  <Name>Alpha-Video</Name>
  <Repository>andrewstech/alpha-video</Repository>
  <Registry>https://hub.docker.com/r/andrewstech/alpha-video</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://discord.gg/78p36SbKcV</Support>
  <Project>https://github.com/unofficial-skills/alpha-video</Project>
  <Overview/>
  <Category>Music</Category>
  <WebUI>http://[IP]:[PORT:5000]</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/Kippenhof/docker-templates/main/alpha-video.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/Kippenhof/docker-templates/main/pictures/yt2thesea.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled/>
  <DonateText/>
  <DonateLink/>
  <Description> An Conatiner, that allows you to stream Youtube via Alexa &#xD;
  I documented the Installation-Process here:  &#xD;
https://alpha-video.andrewstech.me/docs/unraid-os </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5000</HostPort>
        <ContainerPort>5000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9001</HostPort>
        <ContainerPort>9001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/alpha-video</HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>Get Creative!</Value>
      <Name>subdomain</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>true</Value>
      <Name>localtunnel</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Port 5000" Target="5000" Default="5000" Mode="tcp" Description="Container Port: 5000" Type="Port" Display="always" Required="false" Mask="false">5000</Config>
  <Config Name="Port 9001" Target="9001" Default="9001" Mode="tcp" Description="Supervisor Process Control System (Web UI)" Type="Port" Display="always" Required="false" Mask="false">9001</Config>
    <Config Name="dockersock" Target="/data" Default="/data" Mode="rw" Description="Config for bespoken (saves the Generated URL)" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/alpha-video</Config>
  <Config Name="Subdomain " Target="subdomain" Default="changeme" Mode="" Description="deprecated (Will be removed in the Next Patches)" Type="Variable" Display="always" Required="false" Mask="false">Get Creative!</Config>
  <Config Name="Enable Localtunnel" Target="localtunnel" Default="true" Mode="" Description="deprecated (Will be removed in the Next Patches)" Type="Variable" Display="always" Required="true" Mask="false">true</Config>
</Container>
